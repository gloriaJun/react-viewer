{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": [
      "../dist/typings"
    ],
    "baseUrl": "./src/client",
    "paths": {
      "@ridi/react-viewer": ["../../../dist"]
    }
  },
  "exclude": [
    "../node_modules",
    "./node_modules"
  ]
}
